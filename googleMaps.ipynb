    "from geopy.geocoders import Nominatim\n",
    "\n",
    "geolocator = Nominatim(user_agent=\"my-app\")\n",
    "location = geolocator.geocode(\"Nairobi\")\n",
    "\n",
    "print(location.address)\n",
    "print(location.latitude, location.longitude)\n"
